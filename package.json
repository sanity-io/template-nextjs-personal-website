    "@sanity/image-url": "1.1.0",
    "@sanity/react-loader": "1.10.40",
    "@sanity/ui": "2.11.7",
    "@sanity/vision": "3.72.1",
    "@tailwindcss/typography": "0.5.16",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "next": "15.1.6",
    "next-sanity": "9.8.46",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rxjs": "7.8.1",
    "sanity": "3.72.1",
    "sanity-plugin-asset-source-unsplash": "3.0.3",
