    "eslint-plugin-simple-import-sort": "12.1.1",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-packagejson": "2.5.8",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3"
  }
