{
  "name": "sanity-template-template-nextjs-personal-website",
  "private": true,
  "scripts": {
    "analyze": "next experimental-analyze --serve",
    "build": "next build --turbopack && sanity manifest extract --path public/studio/static",
    "predev": "npm run typegen",
    "dev": "next --turbopack",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "lint": "eslint .",
    "lint:fix": "npm run format && npm run lint -- --fix",
    "start": "next start",
    "type-check": "next typegen && tsc --noEmit",
    "typegen": "sanity schema extract && sanity typegen generate"
  },
  "dependencies": {
    "@next/env": "16.1.1",
    "@sanity/client": "7.14.0",
    "@sanity/demo": "2.0.0",
    "@sanity/icons": "3.7.4",
    "@sanity/image-url": "2.0.2",
    "@sanity/ui": "3.1.11",
    "@sanity/util": "5.2.0",
    "@sanity/vision": "5.2.0",
    "@tailwindcss/typography": "0.5.19",
    "@vercel/speed-insights": "1.3.1",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "next": "16.1.1",
    "next-sanity": "13.0.0-cache-components.16",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-live-transitions": "0.2.0",
    "rxjs": "7.8.2",
    "sanity": "5.2.0",
    "sanity-plugin-asset-source-unsplash": "7.0.2",
    "server-only": "0.0.1",
    "sonner": "2.0.7",
    "styled-components": "6.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@sanity/prettier-config": "3.0.0",
    "@types/react": "19.2.7",
    "autoprefixer": "10.4.23",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.2",
    "eslint-formatter-gha": "2.0.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "3.4.19",
    "typescript": "5.9.3"
  }
