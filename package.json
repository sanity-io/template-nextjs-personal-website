{
  "name": "sanity-template-template-nextjs-personal-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build && sanity manifest extract --path public/studio/static",
    "predev": "npm run typegen",
    "dev": "next dev",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "lint": "eslint .",
    "lint:fix": "npm run format && npm run lint -- --fix",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "typegen": "sanity schema extract && sanity typegen generate",
    "update:canaries": "pnpm up next@latest eslint-config-next@latest @next/env@latest next-sanity@canary"
  },
  "dependencies": {
    "@next/env": "16.0.10",
    "@sanity/client": "7.13.2",
    "@sanity/demo": "2.0.0",
    "@sanity/icons": "3.7.4",
    "@sanity/image-url": "1.2.0",
    "@sanity/ui": "3.1.11",
    "@sanity/util": "4.22.0",
    "@sanity/vision": "4.22.0",
    "@tailwindcss/typography": "0.5.19",
    "@vercel/speed-insights": "1.3.1",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "next": "16.0.10",
    "next-sanity": "11.6.12",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-live-transitions": "0.2.0",
    "rxjs": "7.8.2",
    "sanity": "4.22.0",
    "sanity-plugin-asset-source-unsplash": "4.0.1",
    "server-only": "0.0.1",
    "sonner": "2.0.7",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@sanity/prettier-config": "2.0.3",
    "@types/react": "19.2.7",
    "autoprefixer": "10.4.23",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.2",
    "eslint-formatter-gha": "1.6.0",
    "eslint-plugin-react-hooks": "7.0.0",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "3.4.19",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.18.3"
}
